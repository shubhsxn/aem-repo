<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/etc/designs/default"
        cq:lastModified="{Date}2018-09-19T17:12:33.584+05:30"
        cq:lastModifiedBy="admin"
        cq:template="/libs/cq/workflow/templates/model"
        jcr:primaryType="cq:PageContent"
        jcr:title="Basic Deactivation Workflow"
        sling:resourceType="cq/workflow/components/pages/model"
        workflowStages="[accept,reject]">
        <flow
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/parsys">
            <process
                jcr:created="{Date}2018-04-20T22:28:07.571+05:30"
                jcr:createdBy="admin"
                jcr:description="send email to publisher group for the page deactivation review"
                jcr:lastModified="{Date}2018-09-19T17:12:01.171+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                jcr:title="send email to publisher group"
                sling:resourceType="cq/workflow/components/model/process">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    PROCESS="com.corteva.core.workflow.EmailProcessStep"
                    PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/deactivationreviewnotification;receiverList=publisher"
                    PROCESS_AUTO_ADVANCE="true"/>
            </process>
            <dynamic_participant
                jcr:created="{Date}2018-09-19T16:51:55.781+05:30"
                jcr:createdBy="admin"
                jcr:description="review the page and take next steps"
                jcr:lastModified="{Date}2018-09-19T17:12:04.529+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                jcr:title="review the page and take next steps"
                sling:resourceType="cq/workflow/components/model/dynamic_participant">
                <metaData
                    jcr:primaryType="nt:unstructured"
                    DYNAMIC_PARTICIPANT="com.corteva.core.workflow.ParticipantStepImpl"
                    PROCESS_ARGS="role=publisher"/>
            </dynamic_participant>
            <or
                jcr:created="{Date}2018-04-20T22:37:41.374+05:30"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2018-04-20T22:37:41.374+05:30"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/workflow/components/model/or"
                branches="2"
                orSplit="true">
                <_x0031_
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cq/flow/components/parsys">
                    <process
                        jcr:created="{Date}2018-04-20T22:38:01.086+05:30"
                        jcr:createdBy="admin"
                        jcr:description="Approve unpublish request "
                        jcr:lastModified="{Date}2018-09-19T17:12:07.886+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Approve unpublish request "
                        sling:resourceType="cq/workflow/components/model/process">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            PROCESS="com.corteva.core.workflow.AbsoluteTimeProcessStep"
                            PROCESS_AUTO_ADVANCE="true"
                            workflowStage="accept"/>
                    </process>
                    <participant
                        jcr:created="{Date}2018-04-20T22:39:15.741+05:30"
                        jcr:createdBy="admin"
                        jcr:description="waiting for the scheduled time"
                        jcr:lastModified="{Date}2018-09-19T17:12:17.295+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="waiting for the scheduled time"
                        sling:resourceType="cq/workflow/components/model/participant">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            PARTICIPANT="admin"
                            timeoutHandler="com.adobe.granite.workflow.console.timeout.autoadvance.AbsoluteTimeAutoAdvancer"
                            timeoutMillis="0"/>
                    </participant>
                    <process_260908995
                        jcr:created="{Date}2018-04-20T22:41:31.919+05:30"
                        jcr:createdBy="admin"
                        jcr:description="deactivate the page since it has been approved"
                        jcr:lastModified="{Date}2018-09-19T17:12:22.090+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Deactivate the page"
                        sling:resourceType="cq/workflow/components/model/process">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            PROCESS="com.day.cq.wcm.workflow.process.DeactivatePageProcess"
                            PROCESS_AUTO_ADVANCE="true"/>
                    </process_260908995>
                    <process_203923380
                        jcr:created="{Date}2018-04-20T22:43:06.752+05:30"
                        jcr:createdBy="admin"
                        jcr:description="Send mail to initiator regarding the page deactivation"
                        jcr:lastModified="{Date}2018-09-19T17:12:29.156+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Send mail to initiator regarding the page deactivation"
                        sling:resourceType="cq/workflow/components/model/process">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            PROCESS="com.corteva.core.workflow.EmailProcessStep"
                            PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/deactivationacceptnotification"
                            PROCESS_AUTO_ADVANCE="true"/>
                    </process_203923380>
                </_x0031_>
                <_x0032_
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="cq/flow/components/parsys">
                    <process
                        jcr:created="{Date}2018-04-20T22:44:31.056+05:30"
                        jcr:createdBy="admin"
                        jcr:description="Reject unpublish request and notify requestor"
                        jcr:lastModified="{Date}2018-09-19T17:12:33.569+05:30"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Reject unpublish request and notify requestor"
                        sling:resourceType="cq/workflow/components/model/process">
                        <metaData
                            jcr:primaryType="nt:unstructured"
                            PROCESS="com.corteva.core.workflow.EmailProcessStep"
                            PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/deactivationrejectnotification"
                            PROCESS_AUTO_ADVANCE="true"
                            workflowStage="reject"/>
                    </process>
                </_x0032_>
            </or>
        </flow>
        <model/>
        <image
            jcr:lastModified="{Date}2018-04-20T22:37:17.857+05:30"
            jcr:lastModifiedBy="admin"
            jcr:primaryType="nt:unstructured"
            imageRotate="0"/>
    </jcr:content>
</jcr:root>
