            title="Start"
            type="START">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node0>
        <node1
            jcr:primaryType="cq:WorkflowNode"
            description="send email to publisher group for the page review"
            title="send email to publisher group"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.corteva.core.workflow.EmailProcessStep"
                PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/publishreviewnotification;receiverList=publisher"
                PROCESS_AUTO_ADVANCE="true"/>
        </node1>
        <node2
            jcr:primaryType="cq:WorkflowNode"
            description="review the page and take next steps"
            title="review the page and take next steps"
            type="DYNAMIC_PARTICIPANT">
            <metaData
                jcr:primaryType="nt:unstructured"
                DYNAMIC_PARTICIPANT="com.corteva.core.workflow.ParticipantStepImpl"
                PROCESS_ARGS="role=publisher"/>
        </node2>
        <node3
            jcr:primaryType="cq:WorkflowNode"
