        <node4
            jcr:primaryType="cq:WorkflowNode"
            title="OR_JOIN"
            type="OR_JOIN">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node4>
        <node5
            jcr:primaryType="cq:WorkflowNode"
            description="Approver accept the  content changes ,done by author"
            title="Approve page and publish"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.corteva.core.workflow.AbsoluteTimeProcessStep"
                PROCESS_AUTO_ADVANCE="true"
                workflowStage="accept"/>
        </node5>
        <node6
            jcr:primaryType="cq:WorkflowNode"
            description="waiting for the scheduled time"
            title="waiting for the scheduled time"
            type="PARTICIPANT">
            <metaData
                jcr:primaryType="nt:unstructured"
                PARTICIPANT="admin"
                timeoutHandler="com.adobe.granite.workflow.console.timeout.autoadvance.AbsoluteTimeAutoAdvancer"
                timeoutMillis="0"/>
        </node6>
        <node7
            jcr:primaryType="cq:WorkflowNode"
            description="activate the page since it has been approved"
            title="Activate the page"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.day.cq.wcm.workflow.process.ActivatePageProcess"
                PROCESS_AUTO_ADVANCE="true"/>
        </node7>
        <node8
            jcr:primaryType="cq:WorkflowNode"
            description="Send mail to initiator regarding the page publish"
            title="Send mail to initiator regarding the page publish"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.corteva.core.workflow.EmailProcessStep"
                PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/publishacceptnotification"
                PROCESS_AUTO_ADVANCE="true"/>
        </node8>
        <node9
            jcr:primaryType="cq:WorkflowNode"
            description="Approver reject the  content changes done by author"
            title="Reject publish request and notify page author"
            type="PROCESS">
            <metaData
                jcr:primaryType="nt:unstructured"
                PROCESS="com.corteva.core.workflow.EmailProcessStep"
                PROCESS_ARGS="emailTemplate=/etc/notification/email/corteva/publishrejectnotification"
                PROCESS_AUTO_ADVANCE="true"
                workflowStage="reject"/>
        </node9>
        <node10
            jcr:primaryType="cq:WorkflowNode"
            title="End"
            type="END">
            <metaData jcr:primaryType="nt:unstructured"/>
        </node10>
