<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Corteva Video Metadata"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <title
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The title for video ."
                fieldLabel="Video Title"
                name="./videoTitle"
                required="{Boolean}true">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </title>
            <desc
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The description for video ."
                fieldLabel="Video Description"
                name="./videoDescription"
                required="{Boolean}true">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </desc>
            <altText
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The alt text for video ."
                fieldLabel="Video ALT Text "
                name="./videoAltText"
                required="{Boolean}true">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </altText>
            <cqTags
                cq:showOnCreate="{Boolean}true"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/common/tagspicker"
                allowCreate="{Boolean}true"
                cq-msm-lockable="cq:tags"
                fieldLabel="Tags"
                metaType="tags"
                name="./cq:tags"
                rootPath="/etc/tags/corteva"/>
            <captions
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The closed captions for video ."
                fieldLabel="Video Closed Captions"
                name="./videoClosedCaptions">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </captions>
            <transcript
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                defaultValue=" "
                fieldLabel="Video Transcript"
                name="./videoTranscript">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="customtext"/>
            </transcript>
            <thumbnail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                defaultValue=" "
                fieldLabel="Video Thumbnail Image"
                name="./videoImage">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="custom"/>
            </thumbnail>
            <duration
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The duration of video."
                fieldLabel="Duration"
                name="./videoDuration"
                required="{Boolean}true">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </duration>
            <videoSrc
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Attribution/Source"
                name="./videoSource">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </videoSrc>
            <expirationDate
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                data-title="The expiration date for video ."
                displayedFormat="YYYY-MM-DD HH:mm"
                fieldLabel="Expiration Date"
                name="./videoExpirationDate"
                required="{Boolean}true"
                type="datetime">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="datepicker"
                    typeHint="Date"/>
            </expirationDate>
            <rightsRestrictions
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Rights Restrictions"
                name="./videoRightRestrictions">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </rightsRestrictions>
            <businessRestrictions
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The business restrictions for video ."
                fieldLabel="Business Restrictions"
                name="./videoBusinessRestrictions">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </businessRestrictions>
            <regionalRestrictions
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The regional restrictions for video ."
                fieldLabel="Regional Restrictions"
                name="./videoRegionalRestrictions">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </regionalRestrictions>
            <product
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Product"
                name="./videoProduct">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </product>
            <seo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                data-title="The Seo keywords for video."
                fieldLabel="SEO Keywords"
                name="./videoSeoKeywords"
                required="{Boolean}true">
                <granite:data
                    jcr:primaryType="nt:unstructured"
                    metaType="text"/>
            </seo>
        </items>
    </content>
</jcr:root>
