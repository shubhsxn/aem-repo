        jcr:primaryType="sling:MessageEntry"
        sling:key="containerSearch.allContent"
        sling:message="Todo Conteúdo"/>
	<ofText      
        jcr:primaryType="sling:MessageEntry"
        sling:key="containerSearch.ofText"
        sling:message="of"/>
    <button
        jcr:primaryType="sling:MessageEntry"
        sling:key="buttonText"
        sling:message="Fechar"/>
    <copyright
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="copy"
