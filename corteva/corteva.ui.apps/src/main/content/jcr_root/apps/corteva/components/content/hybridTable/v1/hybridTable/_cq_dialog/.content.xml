<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="HybridTable"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Hybrid Table"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <cropIconTile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="corteva/components/content/contextualpathbrowser"
                                contentRootPath="/content/dam/dpagco"
                                fieldDescription="Select Crop Icon Tile"
                                fieldLabel="Crop Icon Tile"
                                name="./cropIconTile"/>
                            <tableTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Enter Table Title"
                                fieldLabel="Table Title"
                                maxlength="{Long}130"
                                name="./tableTitle"
                                required="{Boolean}true"/>
                            <tableData
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="corteva/components/content/contextualpathbrowser"
                                class="isCsv"
                                contentRootPath="/content/dam/dpagco"
                                fieldDescription="Upload Table Data"
                                fieldLabel="Table Data"
                                name="./tableData"/>
                            <paddingOptions
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Select Padding Options"
                                fieldLabel="Padding Options"
                                name="./paddingOptions"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                    <noTopPadding
                                        jcr:primaryType="nt:unstructured"
                                        text="No Top Padding"
                                        value="no-top"/>
                                    <noBottomPadding
                                        jcr:primaryType="nt:unstructured"
                                        text="No Bottom Padding"
                                        value="no-bottom"/>
                                    <noPadding
                                        jcr:primaryType="nt:unstructured"
                                        text="No Padding"
                                        value="no-padding"/>
                                </items>
                            </paddingOptions>
                        </items>
                    </columns>
                </items>
            </tab1>
        </items>
    </content>
</jcr:root>