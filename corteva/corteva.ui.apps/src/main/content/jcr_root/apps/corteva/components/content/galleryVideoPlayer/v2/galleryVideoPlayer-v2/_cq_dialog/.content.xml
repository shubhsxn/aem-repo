<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    cq:lastReplicated="{Date}2018-10-03T09:25:58.585Z"
    cq:lastReplicatedBy="admin"
    cq:lastReplicationAction="Activate"
    jcr:mixinTypes="[cq:ReplicationStatus]"
    jcr:primaryType="nt:unstructured"
    jcr:title="Gallery Video Player"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Video Settings"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <rowType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                class="cq-dialog-custom-dropdown-showhide rowType"
                                cq-dialog-dropdown-showhide-target=".rowType-option-listtype-showhide-target"
                                fieldDescription="Select the Row Type."
                                fieldLabel="Row Type"
                                name="./rowType"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <twoVideos
                                        jcr:primaryType="nt:unstructured"
                                        text="2 Videos"
                                        value="twoVideos"/>
                                    <threeVideos
                                        jcr:primaryType="nt:unstructured"
                                        text="3 Videos"
                                        value="threeVideos"/>
                                </items>
                            </rowType>
                            <videoType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                class="cq-dialog-custom-dropdown-showhide videoType"
                                cq-dialog-dropdown-showhide-target=".videoType-option-listtype-showhide-target"
                                fieldDescription="Select the Video Type."
                                fieldLabel="Video Type"
                                name="./videoType"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <youtube
                                        jcr:primaryType="nt:unstructured"
                                        text="YouTube"
                                        value="youtube"/>
                                    <scene7
                                        jcr:primaryType="nt:unstructured"
                                        text="Scene7"
                                        value="scene7"/>
                                </items>
                            </videoType>
                            <setYoutube
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container"
                                class="videoType-option-listtype-showhide-target foundation-layout-util-vmargin"
                                showhidecustomtargetvalue="youtube">
                                <items jcr:primaryType="nt:unstructured">
                                    <videoDetails
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                        fieldLabel="Video Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            acs-commons-nested=""
                                            name="./videoDetails">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <videoId
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            class="js-focus-out isMandatory"
                                                            fieldDescription="Enter Video Id."
                                                            fieldLabel="Video Id"
                                                            name="./videoId"
                                                            required="{Boolean}true"/>
                                                        <videoTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Provide video title."
                                                            fieldLabel="Video Title"
                                                            maxlength="{Long}50"
                                                            name="./videoTitle"/>
                                                        <videoDescription
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                            fieldDescription="Provide video description."
                                                            fieldLabel="Video Description"
                                                            name="./videoDescription"/>
                                                        <videoAltText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            class="isMandatory"
                                                            fieldDescription="Provide video alt text."
                                                            fieldLabel="Video Alt Text"
                                                            name="./videoAltText"
                                                            required="{Boolean}true"/>
                                                        <videoThumbnail
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            class="isMandatory"
                                                            fieldDescription="Provide video thumbnail image URL. "
                                                            fieldLabel="Video Thumbnail Image"
                                                            name="./videoThumbnail"
                                                            contentRootPath="/content/dam/dpagco"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </videoDetails>
                                </items>
                            </setYoutube>
                            <setScene7
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container"
                                class="videoType-option-listtype-showhide-target foundation-layout-util-vmargin"
                                showhidecustomtargetvalue="scene7">
                                <items jcr:primaryType="nt:unstructured">
                                    <videoDetails
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                        fieldLabel="Video Details">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                            acs-commons-nested=""
                                            name="./videoDetailsS7">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                method="absolute"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <column
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <videoPath
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            assetfieldstobepopulated="\{&quot;videoTitle&quot;:&quot;./videoTitleS7&quot;, &quot;videoDescription&quot;:&quot;./videoDescriptionS7&quot;, &quot;videoAltText&quot;:&quot;./videoAltTextS7&quot;, &quot;videoImage&quot;:&quot;./videoThumbnailS7&quot;}"
                                                            class="isMandatory assetMetadataClass"
                                                            fieldDescription="Select the Scene7 Video path."
                                                            fieldLabel="Video Path"
                                                            fieldstobepopulated="\{&quot;jcr:title&quot;:&quot;./title&quot;}"
                                                            name="./videoPath"
                                                            required="{Boolean}true"
                                                            contentRootPath="/content/dam/dpagco"/>
                                                        <videoTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldDescription="Provide video title."
                                                            fieldLabel="Video Title"
                                                            maxlength="{Long}50"
                                                            name="./videoTitleS7"/>
                                                        <videoDescription
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                            fieldDescription="Provide video description."
                                                            fieldLabel="Video Description"
                                                            name="./videoDescriptionS7"/>
                                                        <videoAltText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            class="isMandatory"
                                                            fieldDescription="Provide video alt text."
                                                            fieldLabel="Video Alt Text"
                                                            name="./videoAltTextS7"
                                                            required="{Boolean}true"/>
                                                        <videoThumbnail
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            class="isMandatory"
                                                            fieldDescription="Provide video thumbnail image URL. "
                                                            fieldLabel="Video Thumbnail Image"
                                                            name="./videoThumbnailS7"
                                                            contentRootPath="/content/dam/dpagco"/>
                                                    </items>
                                                </column>
                                            </items>
                                        </field>
                                    </videoDetails>
                                </items>
                            </setScene7>
                        </items>
                    </columns>
                </items>
            </tab1>
        </items>
    </content>
</jcr:root>
