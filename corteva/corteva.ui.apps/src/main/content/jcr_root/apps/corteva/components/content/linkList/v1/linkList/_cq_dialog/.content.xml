<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Link List"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Links Details"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <separator
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/radiogroup"
                                fieldDescription="Select 'Yes' to show separator between links."
                                fieldLabel="Show Separator?"
                                name="./separator">
                                <items jcr:primaryType="nt:unstructured">
                                    <option1
                                        jcr:primaryType="nt:unstructured"
                                        checked="{Boolean}true"
                                        text="Yes"
                                        value="with-seperator"/>
                                    <option2
                                        jcr:primaryType="nt:unstructured"
                                        text="No"
                                        value="without-seperator"/>
                                </items>
                            </separator>
                            <navLinks
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Navigation Links"
                                maxItemsGnav="5">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested=""
                                    name="./navLinks">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
												<isCountrySel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    fieldDescription="Country selector link"
                                                    fieldLabel="Is this a Country Selector link?"
                                                    name="./countrySel">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <option1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Yes"
                                                            value="yes"/>
                                                        <option2
                                                            jcr:primaryType="nt:unstructured"
                                                            selected="{Boolean}true"
                                                            text="No"
                                                            value="no"/>
                                                    </items>
                                                </isCountrySel>
                                                <linkLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldDescription="Link Label"
                                                    fieldLabel="Link Label"
                                                    maxlength="{Long}30"
                                                    name="./linkLabel"
                                                    required="{Boolean}true"/>
                                                <linkUrl
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                    fieldLabel="Link Url"
                                                    name="./linkUrl"
                                                    required="{Boolean}true"
                                                    contentRootPath="/content"/>
                                                <linkAction
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/include"
                                                    path="/apps/corteva/components/content/commonElements/link/linkAction"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </navLinks>
                        </items>
                    </columns>
                </items>
            </tab1>
        </items>
    </content>
</jcr:root>
