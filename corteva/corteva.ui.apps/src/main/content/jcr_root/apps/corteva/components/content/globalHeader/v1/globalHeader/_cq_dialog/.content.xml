<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <tab1
                jcr:primaryType="nt:unstructured"
                jcr:title="Field Details"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <setLogoImage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container"
                                assetfieldstobepopulated="\{&quot;logoAltText&quot;:&quot;./logoAltText&quot;}"
                                class="assetMetadataClass">
                                <items jcr:primaryType="nt:unstructured">
                                    <logoImage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/include"
                                        path="corteva/components/content/commonElements/image/smartimage"/>
                                    <logoAltText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/include"
                                        path="corteva/components/content/commonElements/logo/logoAltText"/>
                                </items>
                            </setLogoImage>
                            <logoUrl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="corteva/components/content/commonElements/logo/logoUrl"/>
                        </items>
                    </columns>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="nt:unstructured"
                jcr:title="Main Menu Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <navLinks
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/multifield"
                                fieldLabel="Navigation Links"
                                maxItemsGnav="5">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    acs-commons-nested=""
                                    name="./navLinks">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                        method="absolute"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <linkLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Enter Link Label."
                                                    fieldLabel="Link Label"
                                                    maxlength="{Long}26"
                                                    name="./linkLabel"
                                                    required="{Boolean}true"/>
                                                <hasDropdown
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                                    fieldDescription="Choose 'Yes' if main menu link has sub-navigation dropdown."
                                                    fieldLabel="Has Sub-Navigation Dropdown?"
                                                    name="./hasDropdown">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <option1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="No"
                                                            value="no"/>
                                                        <option2
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Yes"
                                                            value="yes"/>
                                                    </items>
                                                </hasDropdown>
                                                <linkUrl
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                    fieldDescription="Provide the Destination Link."
                                                    fieldLabel="Link URL"
                                                    name="./linkUrl"
                                                    contentRootPath="/content"/>
                                                <linkAction
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/include"
                                                    path="/apps/corteva/components/content/commonElements/link/linkAction"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </navLinks>
                        </items>
                    </columns>
                </items>
            </tab2>
        </items>
    </content>
</jcr:root>
