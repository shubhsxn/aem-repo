<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Product Technical Specs"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        jcr:title="Product Technical Specs Settings"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/section">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <specs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Product Technical Specs Configuration">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                    name="./productSpecs">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Section Configuration"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/layouts/collapsible"/>
                                            <items jcr:primaryType="nt:unstructured">
                                                <sectionLabel
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Select the Section Label"
                                                    name="./sectionLabel"
                                                    required="{Boolean}true"
                                                    value="">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <label1
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Active Ingredient"
                                                            value="pdp.label.active.ingredient"/>
                                                        <label2
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Application Method"
                                                            value="pdp.label.application.method"/>
                                                        <label3
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Mixing Instructions"
                                                            value="pdp.label.mixing.instructions"/>
                                                        <label4
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Rates"
                                                            value="pdp.label.rates"/>
                                                        <label5
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Mode of Action"
                                                            value="pdp.label.mode.of.action"/>
                                                        <label6
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Application Rate"
                                                            value="pdp.label.application.rate"/>
                                                        <label7
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Application Timing"
                                                            value="pdp.label.application.timing"/>
                                                        <label8
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Rate of Timing"
                                                            value="pdp.label.rate.of.timing"/>
                                                        <label9
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Formulations"
                                                            value="pdp.label.formulations"/>
                                                        <label10
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Herbicide Group"
                                                            value="pdp.label.herbicide.group"/>
                                                        <label18
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Fungicide Group"
                                                            value="pdp.label.fungicide.group"/>
                                                        <label19
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Insecticide Group"
                                                            value="pdp.label.insecticide.group"/>
                                                        <label11
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Packaging"
                                                            value="pdp.label.packaging"/>
                                                        <label12
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Crop Rotation"
                                                            value="pdp.label.crop.rotation"/>
                                                        <label13
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Hazard Information"
                                                            value="pdp.label.hazard.information"/>
                                                        <label14
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Approved Use"
                                                            value="pdp.label.approved.use"/>
                                                        <label15
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Grazing"
                                                            value="pdp.label.grazing"/>
                                                        <label16
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Tank Mixes"
                                                            value="pdp.label.tank.mixes"/>
                                                        <label17
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Additional Information"
                                                            value="pdp.label.additional.information"/>
                                                    </items>
                                                </sectionLabel>
                                                <setSectionDetail
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Section Detail *"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <sectionDetail
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/include"
                                                            path="/apps/corteva/components/content/commonElements/bodytext"/>
                                                    </items>
                                                </setSectionDetail>
                                                <mediaType
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldDescription="Select the optional Media Type to include an image or a video"
                                                    fieldLabel="Media Type"
                                                    name="./mediaType"
                                                    value="image">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <none
                                                            jcr:primaryType="nt:unstructured"
                                                            text="None"
                                                            value="none"/>
                                                        <image
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Image"
                                                            value="image"/>
                                                        <scene7video
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Scene7 Video"
                                                            value="scene7video"/>
                                                        <youtubevideo
                                                            jcr:primaryType="nt:unstructured"
                                                            text="YouTube Video"
                                                            value="youtubevideo"/>
                                                    </items>
                                                </mediaType>
                                                <setImageFields
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Image Configuration"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <desktopImageUrl
                                                            granite:class="assetMetadataClass"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            fieldDescription="Please configure if selected media type is image"
                                                            fieldLabel="Desktop Image"
                                                            name="./desktopImageUrl"
                                                            contentRootPath="/content/dam/dpagco">
                                                            <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                assetfieldstobepopulated="\{&quot;imageAltText&quot;:&quot;./imgAltText&quot;}"/>
                                                        </desktopImageUrl>
                                                        <imageAltText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please configure if selected media type is image"
                                                            fieldLabel="Image Alt Text"
                                                            name="./imgAltText"/>
                                                    </items>
                                                </setImageFields>
                                                <setVideoFields
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Video Configuration"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <videoId
                                                            granite:class="js-focus-out"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please configure if selected media type is YouTube Video"
                                                            fieldLabel="YouTube Video Id"
                                                            name="./videoId"/>
                                                        <videoUrl
                                                            granite:class="assetMetadataClass"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            fieldDescription="Please configure if selected media type is Scene7 Video"
                                                            fieldLabel="Scene7 Video Path"
                                                            name="./videoUrl"
                                                            contentRootPath="/content/dam/dpagco">
                                                            <granite:data
                                                                jcr:primaryType="nt:unstructured"
                                                                assetfieldstobepopulated="\{&quot;videoTitle&quot;:&quot;./videoTitle&quot;, &quot;videoDescription&quot;:&quot;./videoDescription&quot;, &quot;videoAltText&quot;:&quot;./videoAltText&quot;, &quot;videoImage&quot;:&quot;./videoThumbnail&quot;}"/>
                                                        </videoUrl>
                                                        <videoTitle
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please configure if selected media type is Scene7 or YouTube Video"
                                                            fieldLabel="Video Title"
                                                            name="./videoTitle"/>
                                                        <videoDesc
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                            fieldDescription="Please configure if selected media type is Scene7 or YouTube Video"
                                                            fieldLabel="Video Description"
                                                            name="./videoDescription"/>
                                                        <videoAltText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please configure if selected media type is Scene7 or YouTube Video"
                                                            fieldLabel="Video Alt Text"
                                                            name="./videoAltText"/>
                                                        <thumbnailImage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="corteva/components/content/contextualpathbrowser"
                                                            fieldDescription="Please configure if selected media type is Scene7 or YouTube Video"
                                                            fieldLabel="Video Thumbnail Image"
                                                            name="./videoThumbnail"
                                                            contentRootPath="/content/dam/dpagco"/>
                                                    </items>
                                                </setVideoFields>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </specs>
                        </items>
                    </column>
                </items>
            </section>
        </items>
    </content>
</jcr:root>
