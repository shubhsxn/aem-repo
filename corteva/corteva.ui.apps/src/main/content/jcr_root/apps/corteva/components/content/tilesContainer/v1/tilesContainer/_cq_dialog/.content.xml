<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Tiles Container"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/foundation/container">
		<layout jcr:primaryType="nt:unstructured"
			sling:resourceType="granite/ui/components/foundation/layouts/tabs"
			type="nav" />
		<items jcr:primaryType="nt:unstructured">
			<tab1 jcr:primaryType="nt:unstructured"
				jcr:title="Tiles Container Settings"
				sling:resourceType="granite/ui/components/foundation/container">
				<layout jcr:primaryType="nt:unstructured"
					sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" />
				<items jcr:primaryType="nt:unstructured">
					<columns jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/foundation/container">
						<items jcr:primaryType="nt:unstructured">
							<tileType jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/select"
								class="cq-dialog-custom-dropdown-showhide cardType"
								cq-dialog-dropdown-showhide-target=".tileType-option-listtype-showhide-target"
								fieldDescription="Select the Tile Type."
								fieldLabel="Select the Tile Type" name="./tileType"
								required="{Boolean}true" value="">
								<items jcr:primaryType="nt:unstructured">
									<ctaCardWithArrow
										jcr:primaryType="nt:unstructured"
										text="Tile Quarter Width Full Bleed" value="ctaCardWithArrow" />
									<tileProfile jcr:primaryType="nt:unstructured"
										text="Tile Profile View" value="tileProfile" />
									<tileProduct jcr:primaryType="nt:unstructured"
										text="Tile Product Promo View" value="tileProduct" />
									<tileBrandLogo jcr:primaryType="nt:unstructured"
										text="Tile Brand Logo" value="tileBrandLogo" />
									<tileCropIcon jcr:primaryType="nt:unstructured"
										text="Tile Crop Icon" value="tileCropIcon" />
								</items>
							</tileType>
							<shortTitleSection
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="ctaCardWithArrow, tileProfile, tileProduct">
								<items jcr:primaryType="nt:unstructured">
									<title jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/textfield"
										class="isMandatory" fieldDescription="Container Title"
										fieldLabel="Container Title" maxlength="{Long}50"
										name="./title" required="{Boolean}true" />
								</items>
							</shortTitleSection>
							<expandedTitleSection
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="tileBrandLogo, tileCropIcon">
								<items jcr:primaryType="nt:unstructured">
									<title jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/textfield"
										class="isMandatory" fieldDescription="Container Title"
										fieldLabel="Container Title" maxlength="{Long}130"
										name="./expandedTitle" required="{Boolean}true" />
								</items>
							</expandedTitleSection>
							<bodyText jcr:primaryType="nt:unstructured"
								sling:resourceType="cq/gui/components/authoring/dialog/richtext"
								fieldLabel="Body Text" name="./bodyText"
								useFixedInlineToolbar="{Boolean}true">
								<rtePlugins jcr:primaryType="nt:unstructured">
									<format jcr:primaryType="nt:unstructured" features="*" />
									<justify jcr:primaryType="nt:unstructured" features="*" />
									<lists jcr:primaryType="nt:unstructured" features="*" />
									<paraformat jcr:primaryType="nt:unstructured"
										features="*" />
									<links jcr:primaryType="nt:unstructured" features="*" />
									<image jcr:primaryType="nt:unstructured" features="*" />
									<findreplace jcr:primaryType="nt:unstructured"
										features="*" />
									<subsuperscript
										jcr:primaryType="nt:unstructured" features="*" />
									<spellcheck jcr:primaryType="nt:unstructured"
										features="*" />
									<misctools jcr:primaryType="nt:unstructured"
										features="*">
										<specialCharsConfig
											jcr:primaryType="nt:unstructured">
											<chars jcr:primaryType="nt:unstructured">
												<copyright jcr:primaryType="nt:unstructured"
													entity="&amp;#169;" />
												<trademark jcr:primaryType="nt:unstructured"
													entity="&amp;#8482;" />
												<registered jcr:primaryType="nt:unstructured"
													entity="&amp;#174;" />
												<emDash jcr:primaryType="nt:unstructured"
													entity="&amp;#8212;" />
												<pound jcr:primaryType="nt:unstructured"
													entity="&amp;#163;" />
												<nbsp jcr:primaryType="nt:unstructured"
													entity="&amp;#160;" />
											</chars>
										</specialCharsConfig>
									</misctools>
									<styles jcr:primaryType="nt:unstructured" features="*">
										<styles jcr:primaryType="nt:unstructured">
											<plainSpan jcr:primaryType="nt:unstructured"
												cssName="defaultSpan" text="Default (add span tag)" />
											<lead jcr:primaryType="nt:unstructured" cssName="lead"
												text="Lead" />
										</styles>
									</styles>
									<table jcr:primaryType="nt:unstructured" features="*" />
								</rtePlugins>
								<uiSettings jcr:primaryType="nt:unstructured">
									<cui jcr:primaryType="nt:unstructured">
										<inline jcr:primaryType="nt:unstructured"
											toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
											<popovers jcr:primaryType="nt:unstructured">
												<justify jcr:primaryType="nt:unstructured"
													items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
													ref="justify" />
												<lists jcr:primaryType="nt:unstructured"
													items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
													ref="lists" />
												<paraformat jcr:primaryType="nt:unstructured"
													items="paraformat:getFormats:paraformat-pulldown"
													ref="paraformat" />
												<styles jcr:primaryType="nt:unstructured"
													items="styles:getStyles:styles-pulldown" ref="styles" />
											</popovers>
										</inline>
									</cui>
								</uiSettings>
								<htmlRules jcr:primaryType="nt:unstructured">
									<docType jcr:primaryType="nt:unstructured">
										<typeConfig jcr:primaryType="nt:unstructured"
											useSemanticMarkup="{Boolean}true">
											<semanticMarkupMap
												jcr:primaryType="nt:unstructured" 
												b="strong" 
												i="em"/>
										</typeConfig>
									</docType>
								</htmlRules>
							</bodyText>
							<buttonSection jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="ctaCardWithArrow">
								<items jcr:primaryType="nt:unstructured">
									<buttonText jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/include"
										path="/apps/corteva/components/content/commonElements/button/buttontext" />
									<buttonLink jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/include"
										path="/apps/corteva/components/content/commonElements/button/buttonlink" />
									<buttonAction jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/include"
										path="/apps/corteva/components/content/commonElements/button/buttonAction" />
								</items>
							</buttonSection>
							<setCtaCardWithArrow
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="ctaCardWithArrow">
								<items jcr:primaryType="nt:unstructured">
									<ctaCardWithArrowPaths
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/multifield"
										fieldLabel="Tile Quarter Width Full Bleed Paths" maxItems="4"
										minItems="4">
										<field jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/foundation/form/fieldset"
											acs-commons-nested="" name="./ctaCardWithArrowPaths">
											<layout jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
												method="absolute" />
											<items jcr:primaryType="nt:unstructured">
												<column jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<ctaCardWithArrowPath
															jcr:primaryType="nt:unstructured"
															sling:resourceType="corteva/components/content/contextualpathbrowser"
															cardView="ctaCardWithArrow" class="expfragPath"
															fieldLabel="Tile Quarter Width Full Bleed Path"
															name="./ctaCardWithArrowPath" required="{Boolean}true"
															rootPath="" />
													</items>
												</column>
											</items>
										</field>
									</ctaCardWithArrowPaths>
								</items>
							</setCtaCardWithArrow>
							<setTileBrandLogo
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="tileBrandLogo">
								<items jcr:primaryType="nt:unstructured">
									<tileBrandLogoPaths
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/multifield"
										fieldLabel="Tile Brand Logo Paths" minItems="2">
										<field jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/foundation/form/fieldset"
											acs-commons-nested="" name="./tileBrandLogoPaths">
											<layout jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
												method="absolute" />
											<items jcr:primaryType="nt:unstructured">
												<column jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<tileBrandLogoPath
															jcr:primaryType="nt:unstructured"
															sling:resourceType="corteva/components/content/contextualpathbrowser"
															cardView="tileBrandLogo" class="expfragPath"
															fieldLabel="Tile Brand Logo Path"
															name="./tileBrandLogoPath" required="{Boolean}true"
															rootPath="" />
													</items>
												</column>
											</items>
										</field>
									</tileBrandLogoPaths>
								</items>
							</setTileBrandLogo>
							<setTileCropIcon
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/container"
								class="tileType-option-listtype-showhide-target foundation-layout-util-vmargin"
								showhidecustomtargetvalue="tileCropIcon">
								<items jcr:primaryType="nt:unstructured">
									<bgColor jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/select"
										fieldDescription="Select the Background Color."
										fieldLabel="Background Color" name="./bgColor" value="">
										<items jcr:primaryType="nt:unstructured">
											<blue jcr:primaryType="nt:unstructured" text="Blue"
												value="blue" />
											<orange jcr:primaryType="nt:unstructured"
												text="Orange" value="orange" />
											<green jcr:primaryType="nt:unstructured" text="Green"
												value="green" />
											<maroon jcr:primaryType="nt:unstructured" text="Maroon"
												value="maroon" />
										</items>
									</bgColor>
									<tileCropIconPaths
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/foundation/form/multifield"
										fieldLabel="Tile Crop Icon Paths" maxItems="12" minItems="6">
										<field jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/foundation/form/fieldset"
											acs-commons-nested="" name="./tileCropIconPaths">
											<layout jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
												method="absolute" />
											<items jcr:primaryType="nt:unstructured">
												<column jcr:primaryType="nt:unstructured"
													sling:resourceType="granite/ui/components/foundation/container">
													<items jcr:primaryType="nt:unstructured">
														<tileCropIconPath
															jcr:primaryType="nt:unstructured"
															sling:resourceType="corteva/components/content/contextualpathbrowser"
															cardView="tileCropIcon" class="expfragPath"
															fieldLabel="Tile Crop Icon Path"
															name="./tileCropIconPath" required="{Boolean}true"
															rootPath="" />
													</items>
												</column>
											</items>
										</field>
									</tileCropIconPaths>
								</items>
							</setTileCropIcon>
							<tileResourceHidden
								jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/foundation/form/hidden"
								class="componentHidden" name="./tileResourceHidden" />
						</items>
					</columns>
				</items>
			</tab1>
		</items>
	</content>
</jcr:root>
